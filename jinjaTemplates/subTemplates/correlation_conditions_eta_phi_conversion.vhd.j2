{%- block correlation_conditions_eta_phi_conversion_loop %}
  {%- set objects = menu.reporter['ObjectsForScaleConversion'] %}
  {%- for o in objects %}
    {%- if 'EG' in o.type or 'JET' in o.type or 'TAU' in o.type %}
{{o.type | lower}}_conv_2_muon_bx_{{o.bx | lower}}_l: for i in 0 to NR_{{o.type | upper}}_OBJECTS-1 generate
  {{o.type | lower}}_eta_conv_2_muon_eta_integer_bx_{{o.bx | lower}}(i) <= {{o.type | upper}}_ETA_CONV_2_MUON_ETA_LUT(CONV_INTEGER({{o.type | lower}}_bx_{{o.bx | lower}}(i)(D_S_I_{{o.type | upper}}_V2.eta_high downto D_S_I_{{o.type | upper}}_V2.eta_low)));
  {{o.type | lower}}_phi_conv_2_muon_phi_integer_bx_{{o.bx | lower}}(i) <= {{o.type | upper}}_PHI_CONV_2_MUON_PHI_LUT(CONV_INTEGER({{o.type | lower}}_bx_{{o.bx | lower}}(i)(D_S_I_{{o.type | upper}}_V2.phi_high downto D_S_I_{{o.type | upper}}_V2.phi_low)));
end generate;
    {%- elif 'ETM' in o.type or 'HTM' in o.type or 'ETMHF' in o.type %}
{{o.type | lower}}_conv_2_muon_bx_{{o.bx | lower}}_l: for i in 0 to NR_{{o.type | upper}}_OBJECTS-1 generate
  {{o.type | lower}}_phi_conv_2_muon_phi_integer_bx_{{o.bx | lower}}(i) <= {{o.type | upper}}_PHI_CONV_2_MUON_PHI_LUT(CONV_INTEGER({{o.type | lower}}_bx_{{o.bx | lower}}(D_S_I_{{o.type | upper}}_V2.phi_high downto D_S_I_{{o.type | upper}}_V2.phi_low)));
end generate;
    {%- endif %}
  {%- endfor %}
{%- endblock correlation_conditions_eta_phi_conversion_loop %}
{# eof #}
