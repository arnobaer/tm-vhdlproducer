{%- block instantiate_calo_calo_correlation_condition %}
  {%- set o1 = condition.objects[0] %}
  {%- set o2 = condition.objects[1] %}
{{ condition.vhdl_signal }}_i: entity work.calo_calo_correlation_condition
    generic map(
        {{ condition.objectsInSameBx }},
    {%- if condition.mass.enabled == 'true' and condition.mass.type == condition.mass.InvariantMassDeltaRType %}
        mass_cut => {{ condition.mass.enabled }}, mass_type => {{ condition.mass.type }},
    {%- else %}
        deta_cut => {{ condition.deltaEta.enabled }}, dphi_cut => {{ condition.deltaPhi.enabled }}, dr_cut => {{ condition.deltaR.enabled }}, mass_cut => {{ condition.mass.enabled }}, mass_type => {{ condition.mass.type }}, twobody_pt_cut => {{ condition.twoBodyPt.enabled }},
    {%- endif %}        
        nr_calo1_objects => NR_{{ o1.type|upper }}_OBJECTS;
        calo1_object_low => {{ o1.sliceLow }}, calo1_object_high => {{ o1.sliceHigh }}, et_ge_mode_calo1 => {{ o1.operator }}, obj_type_calo1 => {{ o1.type|upper }}_TYPE,
        et_threshold_calo1 => X"{{ o1.threshold|X04 }}",
        nr_eta_windows_calo1 => {{ o1.etaNrCuts }},
        eta_w1_upper_limit_calo1 => X"{{ o1.etaW1UpperLimit|X04 }}", eta_w1_lower_limit_calo1 => X"{{ o1.etaW1LowerLimit|X04 }}",
        eta_w2_upper_limit_calo1 => X"{{ o1.etaW2UpperLimit|X04 }}", eta_w2_lower_limit_calo1 => X"{{ o1.etaW2LowerLimit|X04 }}",
        eta_w3_upper_limit_calo1 => X"{{ o1.etaW3UpperLimit|X04 }}", eta_w3_lower_limit_calo1 => X"{{ o1.etaW3LowerLimit|X04 }}",
        eta_w4_upper_limit_calo1 => X"{{ o1.etaW4UpperLimit|X04 }}", eta_w4_lower_limit_calo1 => X"{{ o1.etaW4LowerLimit|X04 }}",
        eta_w5_upper_limit_calo1 => X"{{ o1.etaW5UpperLimit|X04 }}", eta_w5_lower_limit_calo1 => X"{{ o1.etaW5LowerLimit|X04 }}",
        phi_full_range_calo1 => {{ o1.phiFullRange }}, phi_w1_upper_limit_calo1 => X"{{ o1.phiW1UpperLimit|X04 }}", phi_w1_lower_limit_calo1 => X"{{ o1.phiW1LowerLimit|X04 }}",
        phi_w2_ignore_calo1 => {{ o1.phiW2Ignore }}, phi_w2_upper_limit_calo1 => X"{{ o1.phiW2UpperLimit|X04 }}", phi_w2_lower_limit_calo1 => X"{{ o1.phiW2LowerLimit|X04 }}",
        iso_lut_calo1 => X"{{ o1.isolationLUT|X01 }}",
        nr_calo2_objects => NR_{{ o2.type|upper }}_OBJECTS;
        calo2_object_low => {{ o1.sliceLow }}, calo2_object_high => {{ o1.sliceHigh }}, et_ge_mode_calo2 => {{ o1.operator }}, obj_type_calo2 => {{ o1.type|upper }}_TYPE,
        et_threshold_calo2 => X"{{ o1.threshold|X04 }}",
        nr_eta_windows_calo2 => {{ o1.etaNrCuts }},
        eta_w1_upper_limit_calo2 => X"{{ o1.etaW1UpperLimit|X04 }}", eta_w1_lower_limit_calo2 => X"{{ o1.etaW1LowerLimit|X04 }}",
        eta_w2_upper_limit_calo2 => X"{{ o1.etaW2UpperLimit|X04 }}", eta_w2_lower_limit_calo2 => X"{{ o1.etaW2LowerLimit|X04 }}",
        eta_w3_upper_limit_calo2 => X"{{ o1.etaW3UpperLimit|X04 }}", eta_w3_lower_limit_calo2 => X"{{ o1.etaW3LowerLimit|X04 }}",
        eta_w4_upper_limit_calo2 => X"{{ o1.etaW4UpperLimit|X04 }}", eta_w4_lower_limit_calo2 => X"{{ o1.etaW4LowerLimit|X04 }}",
        eta_w5_upper_limit_calo2 => X"{{ o1.etaW5UpperLimit|X04 }}", eta_w5_lower_limit_calo2 => X"{{ o1.etaW5LowerLimit|X04 }}",
        phi_full_range_calo2 => {{ o1.phiFullRange }}, phi_w1_upper_limit_calo2 => X"{{ o1.phiW1UpperLimit|X04 }}", phi_w1_lower_limit_calo2 => X"{{ o1.phiW1LowerLimit|X04 }}",
        phi_w2_ignore_calo2 => {{ o1.phiW2Ignore }}, phi_w2_upper_limit_calo2 => X"{{ o1.phiW2UpperLimit|X04 }}", phi_w2_lower_limit_calo2 => X"{{ o1.phiW2LowerLimit|X04 }}",
        iso_lut_calo2 => X"{{ o1.isolationLUT|X01 }}",
    {%- if condition.mass.enabled == 'true' and condition.mass.type == condition.mass.InvariantMassDeltaRType %}
        mass_div_dr_vector_width => {{ o1.type|upper }}_{{ o2.type|upper }}_MASS_DIV_DR_VECTOR_WIDTH;
        mass_div_dr_threshold => X"{{ condition.mass.lower|X21 }}"
    {%- else %}
        diff_eta_upper_limit_vector => X"{{ condition.deltaEta.upper|X08 }}", diff_eta_lower_limit_vector => X"{{ condition.deltaEta.lower|X08 }}",
        diff_phi_upper_limit_vector => X"{{ condition.deltaPhi.upper|X08 }}", diff_phi_lower_limit_vector => X"{{ condition.deltaPhi.lower|X08 }}",
        dr_upper_limit_vector => X"{{ condition.deltaR.upper|X16 }}", dr_lower_limit_vector => X"{{ condition.deltaR.lower|X16 }}",
        mass_upper_limit => X"{{ condition.mass.upper|X16 }}",
        mass_lower_limit => X"{{ condition.mass.lower|X16 }}",
        pt1_width => {{ o1.type|upper }}_PT_VECTOR_WIDTH, 
        pt2_width => {{ o2.type|upper }}_PT_VECTOR_WIDTH, 
        mass_cosh_cos_precision => {{ o1.type|upper }}_{{ o2.type|upper }}_COSH_COS_PRECISION, 
        cosh_cos_width => {{ o1.type|upper }}_{{ o2.type|upper }}_COSH_COS_VECTOR_WIDTH,
        pt_sq_threshold_vector => X"{{ condition.twoBodyPt.threshold|X16 }}", 
        sin_cos_width => CALO_SIN_COS_VECTOR_WIDTH, 
        pt_sq_sin_cos_precision => {{ o1.type|upper }}_{{ o2.type|upper }}_SIN_COS_PRECISION
    {%- endif %}        
    )
    port map(lhc_clk, {{ o1.type|lower }}_bx_{{ o1.bx }}, {{ o2.type|lower }}_bx_{{ o2.bx }},
    {%- if condition.mass.type == condition.mass.InvariantMassDeltaRType %}
        mass_div_dr => {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_mass_div_dr,
    {%- else %}
        diff_eta => diff_{{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_eta_vector, diff_phi => diff_{{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_phi_vector,
        pt1 => {{ o1.type|lower }}_pt_vector_bx_{{ o1.bx }}, pt2 => {{ o2.type|lower }}_pt_vector_bx_{{ o2.bx }},
        cosh_deta => {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_cosh_deta_vector, cos_dphi => {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_cos_dphi_vector,
        cos_phi_1_integer => {{ o1.type|lower }}_cos_phi_bx_{{ o1.bx }}, cos_phi_2_integer => {{ o2.type|lower }}_cos_phi_bx_{{ o2.bx }}, sin_phi_1_integer => {{ o1.type|lower }}_sin_phi_bx_{{ o1.bx }}, sin_phi_2_integer => {{ o2.type|lower }}_sin_phi_bx_{{ o2.bx }},
    {%- endif %}
        condition_o => {{ condition.vhdl_signal }});
{%- endblock instantiate_calo_calo_correlation_condition %}

