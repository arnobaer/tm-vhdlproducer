{%- block instantiate_muon_esums_correlation_condition %}
  {%- set o1 = condition.objects[0] %}
  {%- set o2 = condition.objects[1] %}
{{ condition.vhdl_signal }}_i: entity work.muon_esums_correlation_condition
    generic map(
        {{ o1.operator }},
        X"{{ o1.threshold|X04 }}",
        {{ o1.etaFullRange }}, X"{{ o1.etaW1UpperLimit|X04 }}", X"{{ o1.etaW1LowerLimit|X04 }}",
        {{ o1.etaW2Ignore }}, X"{{ o1.etaW2UpperLimit|X04 }}", X"{{ o1.etaW2LowerLimit|X04 }}",
        {{ o1.phiFullRange }}, X"{{ o1.phiW1UpperLimit|X04 }}", X"{{ o1.phiW1LowerLimit|X04 }}",
        {{ o1.phiW2Ignore }}, X"{{ o1.phiW2UpperLimit|X04 }}", X"{{ o1.phiW2LowerLimit|X04 }}",
        "{{ o1.charge }}", X"{{ o1.qualityLUT|X04 }}", X"{{ o1.isolationLUT|X01 }}",
        {{ o2.operator }}, {{ o2.type|upper }}_TYPE,
        X"{{ o2.threshold|X04 }}",
        {{ o2.phiFullRange }}, X"{{ o2.phiW1UpperLimit|X04 }}", X"{{ o2.phiW1LowerLimit|X04 }}",
        {{ o2.phiW2Ignore }}, X"{{ o2.phiW2UpperLimit|X04 }}", X"{{ o2.phiW2LowerLimit|X04 }}",
        {{ condition.diffPhiUpperLimit }}, {{ condition.diffPhiLowerLimit }}, DETA_DPHI_VECTOR_WIDTH_ALL, MUON_{{ o2.type|upper }}_DPHI_PRECISION
    )
    port map(lhc_clk, mu_bx_{{ o1.bx|lower }}, {{ o2.type|lower }}_bx_{{ o2.bx|lower }},
        diff_{{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx|lower }}_bx_{{ o2.bx|lower }}_phi_vector,
        {{ condition.vhdl_signal }});
{%- endblock instantiate_muon_esums_correlation_condition %}
{# eof #}
