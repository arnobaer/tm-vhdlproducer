{%- block instantiate_calo_muon_correlation_condition %}
  {%- set o1 = condition.objects[0] %}
  {%- set o2 = condition.objects[1] %}
{{ condition.vhdl_signal }}_i: entity work.calo_muon_correlation_condition
    generic map(
    {%- if condition.mass.enabled == 'true' and condition.mass.type == condition.mass.InvariantMassDeltaRType %}
        mass_cut => {{ condition.mass.enabled }}, mass_type => {{ condition.mass.type }},
    {%- else %}
        deta_cut => {{ condition.deltaEta.enabled }}, dphi_cut => {{ condition.deltaPhi.enabled }}, dr_cut => {{ condition.deltaR.enabled }}, mass_cut => {{ condition.mass.enabled }}, mass_type => {{ condition.mass.type }}, twobody_pt_cut => {{ condition.twoBodyPt.enabled }},
    {%- endif %}        
        calo_object_low => {{ o1.sliceLow }}, calo_object_high => {{ o1.sliceHigh }}, et_ge_mode_calo => {{ o1.operator }}, obj_type_calo => {{ o1.type|upper }}_TYPE,
        et_threshold_calo => X"{{ o1.threshold|X04 }}",
        nr_eta_windows_calo => {{ o1.etaNrCuts }},
        eta_w1_upper_limit_calo => X"{{ o1.etaW1UpperLimit|X04 }}", eta_w1_lower_limit_calo => X"{{ o1.etaW1LowerLimit|X04 }}",
        eta_w2_upper_limit_calo => X"{{ o1.etaW2UpperLimit|X04 }}", eta_w2_lower_limit_calo => X"{{ o1.etaW2LowerLimit|X04 }}",
        eta_w3_upper_limit_calo => X"{{ o1.etaW3UpperLimit|X04 }}", eta_w3_lower_limit_calo => X"{{ o1.etaW3LowerLimit|X04 }}",
        eta_w4_upper_limit_calo => X"{{ o1.etaW4UpperLimit|X04 }}", eta_w4_lower_limit_calo => X"{{ o1.etaW4LowerLimit|X04 }}",
        eta_w5_upper_limit_calo => X"{{ o1.etaW5UpperLimit|X04 }}", eta_w5_lower_limit_calo => X"{{ o1.etaW5LowerLimit|X04 }}",
        phi_full_range_calo => {{ o1.phiFullRange }}, phi_w1_upper_limit_calo => X"{{ o1.phiW1UpperLimit|X04 }}", phi_w1_lower_limit_calo => X"{{ o1.phiW1LowerLimit|X04 }}",
        phi_w2_ignore_calo => {{ o1.phiW2Ignore }}, phi_w2_upper_limit_calo => X"{{ o1.phiW2UpperLimit|X04 }}", phi_w2_lower_limit_calo => X"{{ o1.phiW2LowerLimit|X04 }}",
        iso_lut_calo => X"{{ o1.isolationLUT|X01 }}",
        muon_object_low => {{ o2.sliceLow }}, muon_object_high => {{ o2.sliceHigh }}, et_ge_mode_muon => {{ o2.operator }}, obj_type_muon => {{ o2.type|upper }}_TYPE,
        pt_threshold_muon => X"{{ o2.threshold|X04 }}",
        nr_eta_windows_muon => {{ o2.etaNrCuts }},
        eta_w1_upper_limit_muon => X"{{ o2.etaW1UpperLimit|X04 }}", eta_w1_lower_limit_muon => X"{{ o2.etaW1LowerLimit|X04 }}",
        eta_w2_upper_limit_muon => X"{{ o2.etaW2UpperLimit|X04 }}", eta_w2_lower_limit_muon => X"{{ o2.etaW2LowerLimit|X04 }}",
        eta_w3_upper_limit_muon => X"{{ o2.etaW3UpperLimit|X04 }}", eta_w3_lower_limit_muon => X"{{ o2.etaW3LowerLimit|X04 }}",
        eta_w4_upper_limit_muon => X"{{ o2.etaW4UpperLimit|X04 }}", eta_w4_lower_limit_muon => X"{{ o2.etaW4LowerLimit|X04 }}",
        eta_w5_upper_limit_muon => X"{{ o2.etaW5UpperLimit|X04 }}", eta_w5_lower_limit_muon => X"{{ o2.etaW5LowerLimit|X04 }}",
        phi_full_range_muon => {{ o2.phiFullRange }}, phi_w1_upper_limit_muon => X"{{ o2.phiW1UpperLimit|X04 }}", phi_w1_lower_limit_muon => X"{{ o2.phiW1LowerLimit|X04 }}",
        phi_w2_ignore_muon => {{ o2.phiW2Ignore }}, phi_w2_upper_limit_muon => X"{{ o2.phiW2UpperLimit|X04 }}", phi_w2_lower_limit_muon => X"{{ o2.phiW2LowerLimit|X04 }}",
        requested_charge_muon => "{{ o2.charge }}", qual_lut_muon => X"{{ o2.qualityLUT|X04 }}", iso_lut_muon => X"{{ o2.isolationLUT|X01 }}",
        upt_cut_muon => {{ o2.hasUpt }}, upt_upper_limit_muon => X"{{ o2.uptUpperLimit|X04 }}", upt_lower_limit_muon => X"{{ o2.uptLowerLimit|X04 }}", ip_lut_muon => X"{{ o2.impactParameterLUT|X01 }}",
        X"{{ condition.deltaEta.upper|X08 }}", X"{{ condition.deltaEta.lower|X08 }}",
        X"{{ condition.deltaPhi.upper|X08 }}", X"{{ condition.deltaPhi.lower|X08 }}",
        X"{{ condition.deltaR.upper|X16 }}", X"{{ condition.deltaR.lower|X16 }}",
    {%- if condition.mass.enabled == 'true' and condition.mass.type == condition.mass.InvariantMassDeltaRType %}
        nr_calo_objects => NR_{{ o1.type|upper }}_OBJECTS;
        mass_div_dr_vector_width => 84;
        mass_div_dr_threshold => X"{{ condition.mass.lower|X21 }}",
    {%- else %}
        mass_upper_limit => X"{{ condition.mass.upper|X16 }}",
        mass_lower_limit => X"{{ condition.mass.lower|X16 }}",
    {%- endif %}        
        pt1_width => {{ o1.type|upper }}_PT_VECTOR_WIDTH, 
        pt2_width => {{ o2.type|upper }}_PT_VECTOR_WIDTH, 
        mass_cosh_cos_precision => {{ o1.type|upper }}_{{ o2.type|upper }}_COSH_COS_PRECISION, 
        cosh_cos_width => {{ o1.type|upper }}_{{ o2.type|upper }}_COSH_COS_VECTOR_WIDTH,
        pt_sq_threshold_vector => X"{{ condition.twoBodyPt.threshold|X16 }}", 
        sin_cos_width => MUON_SIN_COS_VECTOR_WIDTH, 
        pt_sq_sin_cos_precision => {{ o1.type|upper }}_{{ o2.type|upper }}_SIN_COS_PRECISION
    )
    port map(lhc_clk, {{ o1.type|lower }}_bx_{{ o1.bx }}, {{ o2.type|lower }}_bx_{{ o2.bx }},
    {%- if condition.mass.type == condition.mass.InvariantMassDeltaRType %}
        mass_div_dr => {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_mass_div_dr,
    {%- else %}
        diff_{{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_eta_vector, diff_{{ o1.type|lower }}_{{ o1.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_phi_vector,
        {{ o1.type|lower }}_pt_vector_bx_{{ o1.bx }}, {{ o2.type|lower }}_pt_vector_bx_{{ o2.bx }},
        {{ o1.type|lower }}_upt_vector_bx_{{ o1.bx }}, {{ o2.type|lower }}_upt_vector_bx_{{ o2.bx }},
        {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_cosh_deta_vector, {{ o1.type|lower }}_{{ o2.type|lower }}_bx_{{ o1.bx }}_bx_{{ o2.bx }}_cos_dphi_vector,
        {{ o1.type|lower }}_cos_phi_bx_{{ o1.bx }}, {{ o2.type|lower }}_cos_phi_bx_{{ o2.bx }}, {{ o1.type|lower }}_sin_phi_bx_{{ o1.bx }}, {{ o2.type|lower }}_sin_phi_bx_{{ o2.bx }},
    {%- endif %}
        condition_o => {{ condition.vhdl_signal }});
{%- endblock instantiate_calo_muon_correlation_condition %}
{# eof #}
