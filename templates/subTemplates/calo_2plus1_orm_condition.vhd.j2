{%- block instantiate_calo_2plus1_orm_condition %}
  {%- set o1 = condition.objects[0] %}
  {%- set o2 = condition.objects[1] %}
  {%- set o3 = condition.objects[2] %}
{{ condition.vhdl_signal }}_i: entity work.calo_2plus1_orm_condition
    generic map(
        {{ condition.hasDetaOrmCut }}, {{ condition.hasDphiOrmCut }}, {{ condition.hasDrOrmCut }},
        {{ condition.hasDetaCut }}, {{ condition.hasDphiCut }}, {{ condition.hasDrCut }}, {{ condition.hasMassCut }}, {{ condition.massType }}, {{ condition.hasTwoBodyPtCut }}, 
        {{ o1.sliceLow }}, {{ o1.sliceHigh }}, {{ o1.operator }}, {{ o1.type|upper }}_TYPE,
        (X"{{ o1.threshold|X04 }}", X"{{ o2.threshold|X04 }}", X"0000", X"0000"),
        ({{ o1.etaFullRange }}, {{ o2.etaFullRange }}, false, false),
        (X"{{ o1.etaW1UpperLimit|X04 }}", X"{{ o2.etaW1UpperLimit|X04 }}", X"0000", X"0000"), (X"{{ o1.etaW1LowerLimit|X04 }}", X"{{ o2.etaW1LowerLimit|X04 }}", X"0000", X"0000"),
        ({{ o1.etaW2Ignore }}, {{ o2.etaW2Ignore }}, false, false),
        (X"{{ o1.etaW2UpperLimit|X04 }}", X"{{ o2.etaW2UpperLimit|X04 }}", X"0000", X"0000"), (X"{{ o1.etaW2LowerLimit|X04 }}", X"{{ o2.etaW2LowerLimit|X04 }}", X"0000", X"0000"),
        ({{ o1.phiFullRange }}, {{ o2.phiFullRange }}, false, false),
        (X"{{ o1.phiW1UpperLimit|X04 }}", X"{{ o2.phiW1UpperLimit|X04 }}", X"0000", X"0000"), (X"{{ o1.phiW1LowerLimit|X04 }}", X"{{ o2.phiW1LowerLimit|X04 }}", X"0000", X"0000"),
        ({{ o1.phiW2Ignore }}, {{ o2.phiW2Ignore }}, false, false),
        (X"{{ o1.phiW2UpperLimit|X04 }}", X"{{ o2.phiW2UpperLimit|X04 }}", X"0000", X"0000"), (X"{{ o1.phiW2LowerLimit|X04 }}", X"{{ o2.phiW2LowerLimit|X04 }}", X"0000", X"0000"),
        (X"{{ o1.isolationLUT|X01 }}", X"{{ o2.isolationLUT|X01 }}", X"0", X"0"),
        {{ o3.sliceLow }}, {{ o3.sliceHigh }}, {{ o3.operator }}, {{ o3.type|upper }}_TYPE,
        X"{{ o3.threshold|X04 }}",
        {{ o3.etaFullRange }}, X"{{ o3.etaW1UpperLimit|X04 }}", X"{{ o3.etaW1LowerLimit|X04 }}",
        {{ o3.etaW2Ignore }}, X"{{ o3.etaW2UpperLimit|X04 }}", X"{{ o3.etaW2LowerLimit|X04 }}",
        {{ o3.phiFullRange }}, X"{{ o3.phiW1UpperLimit|X04 }}", X"{{ o3.phiW1LowerLimit|X04 }}",
        {{ o3.phiW2Ignore }}, X"{{ o3.phiW2UpperLimit|X04 }}", X"{{ o3.phiW2LowerLimit|X04 }}",
        X"{{ o3.isolationLUT|X01 }}",
	{%- if o1.type == 'EG' and o3.type in ('JET', 'TAU') %}
        {{ o1.type|upper }}_{{ o3.type|upper }}_DETA_DPHI_PRECISION,
	{%- elif o1.type == 'JET' and o3.type == 'EG'  %}
        {{ o3.type|upper }}_{{ o1.type|upper }}_DETA_DPHI_PRECISION,
	{%- elif o1.type == 'JET' and o3.type == 'TAU' %}
        {{ o1.type|upper }}_{{ o3.type|upper }}_DETA_DPHI_PRECISION,
	{%- elif o1.type == 'TAU' and o3.type in ('EG', 'JET')  %}
        {{ o3.type|upper }}_{{ o1.type|upper }}_DETA_DPHI_PRECISION,
	{%- endif %}
        {{ condition.diffEtaOrmUpperLimit }}, {{ condition.diffEtaOrmLowerLimit }},
        {{ condition.diffPhiOrmUpperLimit }}, {{ condition.diffPhiOrmLowerLimit }},
        {{ condition.deltaROrmUpperLimit }}, {{ condition.deltaROrmLowerLimit }}, 
        DETA_DPHI_VECTOR_WIDTH_ALL, {{ o1.type|upper }}_{{ o1.type|upper }}_DETA_DPHI_PRECISION,
        {{ condition.diffEtaUpperLimit }}, {{ condition.diffEtaLowerLimit }},
        {{ condition.diffPhiUpperLimit }}, {{ condition.diffPhiLowerLimit }},
        {{ condition.deltaRUpperLimit }}, {{ condition.deltaRLowerLimit }}, 
        {{ condition.massUpperLimit }}, {{ condition.massLowerLimit }},
        {{ o1.type|upper }}_{{ o1.type|upper }}_MASS_PRECISION, {{ o1.type|upper }}_PT_VECTOR_WIDTH, {{ o1.type|upper }}_{{ o1.type|upper }}_COSH_COS_PRECISION, {{ o1.type|upper }}_{{ o1.type|upper }}_COSH_COS_VECTOR_WIDTH,
        {{ condition.twoBodyPtThres }}, CALO_SIN_COS_VECTOR_WIDTH, {{ o1.type|upper }}_{{ o1.type|upper }}_PT_PRECISION, {{ o1.type|upper }}_{{ o1.type|upper }}_SIN_COS_PRECISION
    )
    port map(lhc_clk, {{ o1.type|lower }}_bx_{{ o1.bx }}, {{ o3.type|lower }}_bx_{{ o3.bx }},
	{%- if o1.type == 'EG' and o3.type in ('JET', 'TAU') %}
	diff_{{ o1.type|lower }}_{{ o3.type|lower }}_bx_{{ o1.bx }}_bx_{{ o3.bx }}_orm_eta_vector, diff_{{ o1.type|lower }}_{{ o3.type|lower }}_bx_{{ o1.bx }}_bx_{{ o3.bx }}_orm_phi_vector,
	{%- elif o1.type == 'JET' and o3.type == 'EG'  %}
	diff_{{ o3.type|lower }}_{{ o1.type|lower }}_bx_{{ o3.bx }}_bx_{{ o1.bx }}_orm_eta_vector, diff_{{ o3.type|lower }}_{{ o1.type|lower }}_bx_{{ o3.bx }}_bx_{{ o1.bx }}_orm_phi_vector,
	{%- elif o1.type == 'JET' and o3.type == 'TAU' %}
	diff_{{ o1.type|lower }}_{{ o3.type|lower }}_bx_{{ o1.bx }}_bx_{{ o3.bx }}_orm_eta_vector, diff_{{ o1.type|lower }}_{{ o3.type|lower }}_bx_{{ o1.bx }}_bx_{{ o3.bx }}_orm_phi_vector,
	{%- elif o1.type == 'TAU' and o3.type in ('EG', 'JET')  %}
	diff_{{ o3.type|lower }}_{{ o1.type|lower }}_bx_{{ o3.bx }}_bx_{{ o1.bx }}_orm_eta_vector, diff_{{ o3.type|lower }}_{{ o1.type|lower }}_bx_{{ o3.bx }}_bx_{{ o1.bx }}_orm_phi_vector,
	{%- endif %}
        diff_{{ o1.type|lower }}_{{ o1.type|lower }}_bx_{{ o1.bx }}_bx_{{ o1.bx }}_eta_vector, diff_{{ o1.type|lower }}_{{ o1.type|lower }}_bx_{{ o1.bx }}_bx_{{ o1.bx }}_phi_vector,
        {{ o1.type|lower }}_pt_vector_bx_{{ o1.bx }},
        {{ o1.type|lower }}_{{ o1.type|lower }}_bx_{{ o1.bx }}_bx_{{ o1.bx }}_cosh_deta_vector,
        {{ o1.type|lower }}_{{ o1.type|lower }}_bx_{{ o1.bx }}_bx_{{ o1.bx }}_cos_dphi_vector,
        {{ o1.type|lower }}_cos_phi_bx_{{ o1.bx }},
        {{ o1.type|lower }}_sin_phi_bx_{{ o1.bx }},
        {{ condition.vhdl_signal }});
{%- endblock instantiate_calo_2plus1_orm_condition %}
{# eof #}
