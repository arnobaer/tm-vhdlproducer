        {{calomuonCondDict['PhiW2Ignore1']}}, X"{{calomuonCondDict['PhiW2UpperLimit1']|X04}}", X"{{calomuonCondDict['PhiW2LowerLimit1']|X04}}",  
	X"{{calomuonCondDict['IsoLut1']|X01}}",	
